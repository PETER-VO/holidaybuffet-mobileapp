{
	"expo": {
		"name": "HolidayBuffet",
		"slug": "HolidayBuffet",
		"version": "2.0.1",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash-app.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"buildNumber": "2",
			"bundleIdentifier": "com.holidaybuffet.restaurant"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.holidaybuffet.restaurant",
			"useNextNotificationsApi": true,
			"googleServicesFile": "./google-services.json",
			"versionCode": 3,
			"permissions": [
				"CAMERA",
				"READ_CONTACTS",
				"RECORD_AUDIO",
				"READ_PHONE_STATE"
			]
		},
		"notification": {
			"icon": "./assets/icon.png",
			"color": "#000000",
			"androidMode": "default",
			"androidCollapsedTitle": "HolidayBuffet",
			"iosDisplayInForeground": true
		}
	}
}
