{
	"expo": {
		"name": "HolidayBuffet",
		"slug": "HolidayBuffet",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon-restaurant-96x96.png",
		"splash": {
			"image": "./assets/restaurant-splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/icon-restaurant-96x96.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.phongvodeveloper.MealsToGo",
			"useNextNotificationsApi": true,
			"googleServicesFile": "./google-services.json"
		},
		"notification": {
			"icon": "./assets/icon-restaurant-96x96.png",
			"color": "#000000",
			"androidMode": "default",
			"androidCollapsedTitle": "HolidayBuffet",
			"iosDisplayInForeground": true
		},
		"plugins": [
			[
				"expo-notifications",
				{
					"icon": "./assets/icon-restaurant-96x96.png",
					"color": "#ffffff",
					"mode": "production"
				}
			]
		]
	}
}
