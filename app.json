{
	"expo": {
		"name": "HolidayBuffet",
		"slug": "HolidayBuffet",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon-app.png",
		"splash": {
			"image": "./assets/splash-app.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/icon-app.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.holidaybuffet.restaurant",
			"useNextNotificationsApi": true,
			"googleServicesFile": "./google-services.json"
		},
		"notification": {
			"icon": "./assets/icon-app.png",
			"color": "#000000",
			"androidMode": "default",
			"androidCollapsedTitle": "HolidayBuffet",
			"iosDisplayInForeground": true
		}
	}
}
