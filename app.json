{
	"expo": {
		"name": "MealsToGo",
		"slug": "MealsToGo",
		"version": "0.0.1",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.phongvodeveloper.MealsToGo"
		},
		"notification": {
			"icon": "./assets/icon.png",
			"color": "#311b92",
			"iosDisplayInForeground": true,
			"androidMode": "default",
			"androidCollapsedTitle": "Updates from Colab.notes"
		}
	}
}
